{
    "$schema": "http://json.schemastore.org/template",
    "author": "Liuhll",
    "classifications": [
        "Silky",
        "MicroService",
        "Web",
        "Gateway"
      ],
      "name": "Silky Gateway",
      "identity": "Silky.Gateway.Template",
      "groupIdentity": "Silky.Gateway.Template",
      "shortName": "silky.gateway",
      "tags": {
        "language": "C#",
        "type": "project"
      },
      "sourceName": "SilkyApp",
      "preferNameDirectory": true,
      "symbols": {
        "rpcToken": {
          "type": "parameter",
          "defaultValue": "ypjdYOzNd4FwENJiEARMLWwK0v7QUHPW",
          "replaces": "rpcTokenValue",
          "isRequired": false,
          "description": "The rpc token for gateway"
        },
        "registrycentertype": {
          "type": "parameter",
          "defaultValue": "Zookeeper",
          "replaces": "registrycentertype",
          "isRequired": false,
          "description": "The registrycenter type for gateway"
        },
        "registrycenterconnections": {
          "type": "parameter",
          "defaultValue": "127.0.0.1:2181,127.0.0.1:2182,127.0.0.1:2183;127.0.0.1:2184,127.0.0.1:2185,127.0.0.1:2186",
          "replaces": "registrycenterconnections",
          "isRequired": false,
          "description": "The registrycenter connectionStrings for gateway"
        },
        "redisenable":{
          "type": "parameter",
          "defaultValue": "true",
          "replaces": "redisenable",
          "isRequired": false,
          "description": "Set distributedcache redis is enable"
        },
        "redisconfiguration":{
          "type": "parameter",
          "defaultValue": "redisconfiguration",
          "replaces": "127.0.0.1:6379,defaultDatabase=0",
          "isRequired": false,
          "description": "Set distributedcache configuration"
        },
        "gatewayjwtsecret":{
          "type": "parameter",
          "defaultValue": "jv1PZkwjLVCEygM7faLLvEhDGWmFqRUW",
          "replaces": "gatewayjwtsecret",
          "isRequired": false,
          "description": "Set gateway jwtsecret configuration"
        },
        "dockersupport": {
         "type": "parameter",
         "datatype": "bool",
         "defaultValue": "true",
         "isRequired": false,
        "description": "Add docker support for Silky"
       },
       "incudeinfrastr": {
        "type": "parameter",
        "datatype": "bool",
        "defaultValue": "true",
        "isRequired": false,
       "description": "Contains infrastr services"
      },
      "dotnetenv": {
        "type": "parameter",
        "datatype": "string",
        "defaultValue": "Development",
        "replaces": "dotnetenv",
        "isRequired": false,
        "description": "Set dotnet env"
      },
      "dockernetwork": {
        "type": "parameter",
        "datatype": "string",
        "defaultValue": "silky_service_net",
        "replaces": "silky_service_net",
        "isRequired": false,
        "description": "Set docker network"
      }
    },
    "sources": [
      {
        "modifiers": [
          {
            "condition": "(dotnetenv!=Apollo)",
            "exclude": [
              "gateways/SilkyApp.GatewayHost/appsettings.Apollo.yml"
            ]
          },
          {
            "condition": "(dotnetenv==Apollo)",
            "exclude": [
              "gateways/SilkyApp.GatewayHost/appsettings.Development.yml",
              "gateways/SilkyApp.GatewayHost/appsettings.Production.yml",
              "gateways/SilkyApp.GatewayHost/appsettings.ContainerDev.yml"
            ]
          },
          {
            "condition": "!(incudeinfrastr)",
            "exclude": [
              "docker-compose/infrastr/**"
            ]
          },
          {
            "condition": "!(dockersupport)",
            "exclude": [
              "SilkyApp.DockerCompose.sln",
              "docker-compose/SilkyApp/**"
            ]
          }
        ]
      }
    ]
}